<h3 class="subheader">Nuevo Registro</h3>
<hr />
<%= simple_form_for @registro_prestacion, :url => proveedor_registro_prestaciones_path(@proveedor) do |f| %>
<%= render :partial =>'form', :locals => { :f => f } %>
<div class="row">
	<div class="text-center">
      <%= link_to 'Cancelar', proveedores_path, :class => "small button" %>		
      <%= f.button :submit, :class => "small button" %>		
	</div>
</div>
<% end %>
