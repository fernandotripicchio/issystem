
<div class="row">
	<h3  class="subheader">Proveedor</h3>
</div>
<div class="panel">
	<div class="row">
		<div class="column large-3">
			<%= label_column("Razon Social:") %>
		</div>
		<div class="column large-9">
			<%= @proveedor.razon_social %>
		</div>
	</div>

	<div class="row">
		<div class="column large-3">
			<%= label_column("Direccion:") %>
		</div>
		<div class="column large-9">
			<%= @proveedor.direccion %>
		</div>
	</div>

	<div class="row">
		<div class="column large-3">
			<%= label_column("CUIT:") %>
		</div>
		<div class="column large-9">
			<%= @proveedor.cuit %>
		</div>
	</div>
	
	<hr />

	<div class="row">
		<div class="text-center">
			<%= link_to 'Editar', edit_proveedor_path(@proveedor), :class => "button small" %> |
			<%= link_to 'Volver', proveedores_path, :class => "button small" %>
		</div>
	</div>
	
